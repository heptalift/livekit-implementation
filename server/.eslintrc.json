{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["google", "eslint:recommended", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "require-jsdoc": "off",
    "no-unused-vars": "warn",
    "new-cap": "off"
  }
}
